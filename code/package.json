{
  "name": "nmos-device-control-mock",
  "version": "0.0.1",
  "description": "",
  "main": "index.js",
  "scripts": {
    "clean": "rimraf dist",
    "copy-deps": "copy-newer --cwd node_modules ** ./dist/node_modules",
    "copy-config": "copy-newer --cwd config ** ./dist/server",
    "start": "node dist/server/Server.js",
    "preserve": "npm run build",
    "s": "npm run serve",
    "serve": "concurrently \"npm run build.w\" \"npm run copy-config\" \"nodemon --watch ./dist/server ./dist/server/Server.js\"",
    "lint": "tslint \"src/**/*.ts\"",
    "prebuild": "npm install --no-optional",
    "b": "npm run build.w",
    "build": "npm run copy-deps && tsc",
    "build.w": "npm run copy-deps && tsc -w"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "axios": "^0.24.0",
    "express": "^4.17.1",
    "json-ignore": "^0.4.0",
    "sdp-transform": "^2.14.1",
    "tai64": "^1.0.0",
    "timestamp-nano": "^1.0.0",
    "uuid": "^8.3.2",
    "ws": "^7.5.6"
  },
  "devDependencies": {
    "@types/express": "^4.17.13",
    "@types/node": "^16.11.12",
    "@types/sdp-transform": "^2.4.5",
    "@types/uuid": "^8.3.1",
    "@types/ws": "^7.4.7",
    "concurrently": "^6.4.0",
    "copy-newer": "^2.1.2",
    "nodemon": "^2.0.15",
    "rimraf": "^3.0.2",
    "typescript": "^4.5.2"
  }
}
